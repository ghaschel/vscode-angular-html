{
  "search.exclude": {
    "out": true
  },
  "eslint.options": {
    "extensions": [
      ".ts"
    ]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "html.format.extraLiners": "head, body, /html, fieldset",
  "html.format.wrapAttributes": "force-expand-multiline",
  "html.format.indentInnerHtml": true,
  "stylelint.snippet": [
    "css",
    "postcss",
    "stylus",
    "styl",
    "scss",
    "sass",
    "less",
    "html"
  ],
  "scss.validate": false,
  "css.validate": false,
  "less.validate": false,
  "stylelint.enable": true,
  "stylelint.packageManager": "npm",
  "stylelint.configFile": ".stylelintrc",
  "stylelint.validate": [
    "html"
  ],
  "html.autoCreateQuotes": false
  //   "postcss",
  //   "less",
  //   "pcss",
  //   "stylus",
  //   "styl",
  //   "css",
  //   "sass",
  //   "scss"
  // ]
}